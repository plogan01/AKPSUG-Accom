[~[tlist_sql;SELECT 
      a.id,
      a.yearid, 
      TO_CHAR(a.yearid + 1991) AS Year,
      a.testcode
    FROM U_AKPSUG_AKTEST_PREF a ORDER BY a.yearid DESC]
  {
    "id": "~(id;json)",
    "yearid": "~(yearid;json)",
    "year": "~(year;json)",
    "testcode": "~(testcode;json)"
  },
[/tlist_sql]{}]